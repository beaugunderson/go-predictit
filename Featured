<script>
    var menuRight = document.getElementById('cbp-spmenu-s2'),
				showRightPush = document.getElementById('showRightPush'),
                showRightPushOpen = document.getElementById('showRightPushOpen'),
				body = document.body;

    showRightPush.onclick = function () {
        classie.toggle(this, 'active');
        classie.toggle(body, 'cbp-spmenu-push-toleft');
        classie.toggle(menuRight, 'cbp-spmenu-open');
        disableOther('showRightPush');
    };

    showRightPushOpen.onclick = function () {
        classie.toggle(this, 'active');
        classie.toggle(body, 'cbp-spmenu-push-toleft');
        classie.toggle(menuRight, 'cbp-spmenu-open');
        disableOther('showRightPush');
    };

    function disableOther(button) {
        if (button !== 'showRightPush') {
            classie.toggle(showRightPush, 'disabled');
        }
    }





    //$('.panel').on('show.bs.collapse', function () {
    //    $(this).find('.accord').addClass('accord-active').addClass('accord-arrow-open').find('.accord-arrow').removeClass('right_arrow').addClass('down_arrow');
    //});
    //$('.panel').on('hide.bs.collapse', function () {
    //    $(this).find('.accord').removeClass('accord-active').removeClass('accord-arrow-open').find('.accord-arrow').removeClass('down_arrow').addClass('right_arrow');
    //});
</script>

    <script type="text/javascript">
        //Close slide out if open on browser re-size
        $(window).resize(function () {
            if ($('#showRightPush').hasClass('active')) {
                showRightPush.click();
            }
        });
    </script>


