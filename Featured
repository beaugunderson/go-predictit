    <link href="/Content/css/ticker_black.css" rel="stylesheet"/>
    <script src="/Scripts/newsticker.jquery.min.js"></script>
    <div id="tickerSection"></div>
    <script>
        var tickerActive = true;
        var tickerLoadCount = 0;
        var tickerLoadCountStop = 5;
        $(function() {
            loadTicker();
        });
    </script>
    <script type="text/javascript">
        function loadTicker() {
            if (tickerActive) {

                tickerLoadCount++;

                $.ajax({
                    url: '/PublicData/_Ticker',
                    type: "GET",
                    traditional: true,
                    success: function(result) {
                        $('#tickerSection').html(result);
                        $('#newsticker_1').newsticker();


                        if (tickerLoadCount >= tickerLoadCountStop) {
                            tickerActive = false;
                            $('#ticker').remove();
                        } else {
                            setTimeout('loadTicker()', 60000);
                        }

                    },
                    error: function() {
                    }
                });
            }
        }

        function removeTicker() {
            $('#ticker').remove();
            tickerActive = false;
            $.ajax({
                type: 'GET',
                cache: 'false',
                url: '/Home/_RemoveTicker',
                traditional: true,
                success: function() {},
                error: function() {}
            });

        }


    </script>

